pipeline{
agent any
stages
{
stage('Build')
{
steps{
echo "Building the code...."
bat "mvn clean"
}
}
stage('Test')
{
steps{
echo "Testing the code...."
bat "mvn clean"
}
}
stage('compile')
{
steps{
echo "Compiling the Project...."
bat "mvn clean"
}
}
stage('Build')
{
steps{
echo "Building the code...."
bat "mvn clean"
}
}
stage('Deploy')
{
steps{
echo "Deploying the code...."
}
}
}
}
